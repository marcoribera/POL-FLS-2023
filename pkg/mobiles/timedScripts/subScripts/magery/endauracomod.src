use uo;
use os;

include ":attributes:attributes";

program TimedScript_Clumsy(params)
	var mobile := params[1];

	if (GetObjProperty(mobile, "auraCoMod") != error)
		var mod := GetObjProperty(mobile, "auraCoMod");
		EraseObjProperty(mobile, "auraCoMod");
		mobile.resist_cold_mod := mobile.resist_cold_mod - mod;
	endif

	return 1;
endprogram